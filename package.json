{
    "name": "senior-citizen-portal-backend",
    "version": "1.0.0",
    "description": "Enterprise backend for Delhi Police Senior Citizen Portal",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "npx prisma generate && npx tsc",
        "postinstall": "prisma generate",
        "start": "node dist/server.js",
        "lint": "eslint src --ext .ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "prisma:studio": "prisma studio",
        "db:export": "ts-node scripts/export-from-supabase.ts",
        "db:import": "ts-node scripts/import-to-neon.ts",
        "db:import-sql": "ts-node scripts/import-sql-to-neon.ts",
        "prisma:prod": "dotenv -e .env.production -- prisma",
        "prisma:dev": "dotenv -e .env.development -- prisma",
        "seed:users": "ts-node prisma/seeds/userSeed.ts",
        "seed:citizens": "ts-node prisma/seeds/citizenSeed.ts",
        "seed:comprehensive": "ts-node prisma/seeds/comprehensive-seed.ts",
        "seed:geojson": "ts-node scripts/seed-from-geojson.ts",
        "seed:all": "npm run seed:users && npm run seed:citizens",
        "migrate:permissions": "ts-node scripts/migrate-permissions-complete.ts"
    },
    "keywords": [
        "express",
        "typescript",
        "postgresql",
        "prisma"
    ],
    "author": "Delhi Police",
    "license": "UNLICENSED",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.974.0",
        "@aws-sdk/s3-request-presigner": "^3.974.0",
        "@prisma/client": "^5.22.0",
        "@sentry/node": "^7.91.0",
        "@sentry/profiling-node": "^1.3.2",
        "@types/json2csv": "^5.0.7",
        "@types/multer": "^2.0.0",
        "@types/pdfkit": "^0.17.4",
        "axios": "^1.13.2",
        "bcryptjs": "^2.4.3",
        "bull": "^4.16.5",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "csurf": "^1.11.0",
        "csv-parser": "^3.2.0",
        "dotenv": "^16.3.1",
        "exceljs": "^4.4.0",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "helmet": "^7.1.0",
        "ioredis": "^5.8.2",
        "json2csv": "^6.0.0-alpha.2",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "multer": "^2.0.2",
        "node-cron": "^4.2.1",
        "pdfkit": "^0.17.2",
        "redis": "^4.6.11",
        "socket.io": "^4.8.1",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/bull": "^4.10.4",
        "@types/compression": "^1.8.1",
        "@types/cookie-parser": "^1.4.10",
        "@types/cors": "^2.8.17",
        "@types/csurf": "^1.11.5",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/morgan": "^1.9.10",
        "@types/node": "^20.10.6",
        "@types/node-cron": "^3.0.11",
        "@types/supertest": "^6.0.3",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "@typescript-eslint/eslint-plugin": "^6.16.0",
        "@typescript-eslint/parser": "^6.16.0",
        "@vercel/node": "^5.5.28",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "jest-mock-extended": "^4.0.0",
        "prisma": "^5.22.0",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.5",
        "ts-node": "^10.9.2",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    }
}
