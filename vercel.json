{
    "version": 2,
    "buildCommand": "npm run build",
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["prisma/schema.prisma", "node_modules/.prisma/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.ts"
        }
    ]
}
